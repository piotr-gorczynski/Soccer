{
  "rules": {
    "status": {
      "$uid": {
        ".read"  : "auth != null && auth.uid == $uid",
        ".write" : "auth != null && auth.uid == $uid"
      }
    },

    "connection-stay-awake": {
      ".read"  : "auth != null",
      ".write" : "auth != null"
    },

    ".read"  : false,
    ".write" : false
  }
}

